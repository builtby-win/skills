{
  "name": "@builtby.win/worktree",
  "version": "0.1.4",
  "description": "Git worktree management with SQLite database snapshots for parallel development",
  "keywords": ["git", "worktree", "sqlite", "database", "snapshot", "development", "parallel"],
  "author": "builtby-win",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/builtby-win/skills"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "worktree": "./dist/cli.js"
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc && chmod +x dist/cli.js",
    "dev": "tsc --watch",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --write .",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@builtby.win/configs": "^1.0.1",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
